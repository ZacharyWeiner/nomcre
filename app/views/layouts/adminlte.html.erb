      <%= render partial: 'shared/adminlte/navigation/top_menu/top_navigation' %>
      <%= render partial: 'shared/adminlte/navigation/left_menu/left_menu' %>
      <div class="content-wrapper">
        <%= render partial: 'shared/adminlte/content_header' %>
        <section class="content">
          <%= render partial: 'shared/adminlte/messages' %>
          <%= yield %>
        </section>
      </div>
      <%= render partial: 'shared/adminlte/footer' %>
    </div>
  </body>

  <%= javascript_include_tag 'application_back' %>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>
  <% if content_for?(:added_scripts) %>
    <%= yield :added_scripts %>
  <% end %>
</html>

