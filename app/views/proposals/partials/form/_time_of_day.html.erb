<div class="panel box box-primary">
  <div class="box-header with-border">
    <h4 class="box-title">Time Of Day</h4>
  </div>
  <div class="box-body">
    <%= radio_button_tag 'proposal[time_of_day]', 'Sunrise', checked = (@proposal.time_of_day =="Sunrise") %>
    <%= f.label :sunrise %>
    <%= radio_button_tag 'proposal[time_of_day]', 'Sunset', checked = (@proposal.time_of_day =="Sunset") %>
    <%= f.label :sunset %>
    <%= radio_button_tag 'proposal[time_of_day]', 'Daylight', checked = (@proposal.time_of_day =="Daylight") %>
    <%= f.label :daylight %>
    <%= radio_button_tag 'proposal[time_of_day]', 'Dusk', checked = (@proposal.time_of_day =="Dusk") %>
    <%= f.label :dusk %>
    <%= radio_button_tag 'proposal[time_of_day]', 'Night', checked = (@proposal.time_of_day =="Night") %>
    <%= f.label :night %>
    <%= radio_button_tag 'proposal[time_of_day]', 'Any', checked = (@proposal.time_of_day =="Any") %>
    <%= f.label :any %>
  </div>
</div>
