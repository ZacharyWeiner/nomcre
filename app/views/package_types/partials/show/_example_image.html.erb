<div class="nk-box bg-dark-1 text-white">
    <div class="bg-image-row row">
        <div class="offset-lg-6 col-lg-6">
            <div class="bg-image bg-image-parallax" style="background-image: url('<%= @package_type.example_image %>');"></div>
        </div>
    </div>
    <div class="container">
      <div class='nk-gap-4'></div>
        <div class="row">
            <div class="col-md-6 ">

                <h2 class="h2 nk-title">
                     <%= image_tag "https://s3-us-west-2.amazonaws.com/nomcre/logos/UpperLeftHPlogo+2.png", style: 'max-height:25px; max-width:25px; border-radius:100%' %>
                    <%= @package_type.example_image_title %>
                </h2>
                <h3 class="nk-sub-title"><%= @package_type.example_image_subtitle %></h3>
                <div class="nk-gap"></div>
                <p><%= simple_format(@package_type.example_image_description)%></p>
            </div>
        </div>
        <div class='nk-gap-4'></div>
    </div>
</div>
