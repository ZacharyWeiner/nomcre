
<% @user_profile.activities.reverse.each do |activity|%>
    <%if activity.activity_type == UserActivityType.collection_added %>
      <%= render :partial => 'user_profiles/partials/black_dashboard/activity/collection_added', :locals => {:activity => activity} %>
    <% elsif  activity.activity_type == UserActivityType.collection_photo_added %>
     <%= render :partial => 'user_profiles/partials/black_dashboard/activity/collection_photo_added', :locals => {:activity => activity} %>
    <% elsif  activity.activity_type == UserActivityType.collection_video_added %>
      <%= render :partial => 'user_profiles/partials/black_dashboard/activity/collection_video_added', :locals => {:activity => activity} %>
    <% elsif  activity.activity_type == UserActivityType.proposal_accepted %>
      <%= render :partial => 'user_profiles/partials/black_dashboard/activity/proposal_accepted', :locals => {:activity => activity, :action_text => 'Accepted'} %>
    <% elsif  activity.activity_type == UserActivityType.proposal_created %>
       <%= render :partial => 'user_profiles/partials/black_dashboard/activity/proposal_accepted', :locals => {:activity => activity, :action_text => 'Created'} %>
    <% elsif  activity.activity_type == UserActivityType.travel_plan_created %>
      <%= render :partial => 'user_profiles/partials/black_dashboard/activity/travel_plan_created', :locals => {:activity => activity} %>
    <% else %>
    <% end %>
  <% end %>
